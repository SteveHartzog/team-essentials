{
    "name": "team-essentials",
    "displayName": "Team Essentials",
    "version": "0.2.10",
    "author": "SteveHartzog",
    "publisher": "SteveHartzog",
    "description": "Provides the essentials tools for any team.",
    "homepage": "https://github.com/SteveHartzog/team-essentials/blog/master/README.md",
    "bugs": "https://github.com/SteveHartzog/team-essentials/issues",
    "license": "SEE LICENSE IN LICENSE.txt",
    "badges": [
        {
            "url": "https://david-dm.org/SteveHartzog/team-essentials.svg",
            "href": "https://david-dm.org/SteveHartzog/team-essentials",
            "description": "Dependency Status"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/SteveHartzog/team-essentials"
    },
    "engines": {
        "vscode": "^1.7.1"
    },
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#3B3738",
        "theme": "dark"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:teamEssentials.changeWindowsShell",
        "onCommand:teamEssentials.filterExplorer",
        "onCommand:teamEssentials.updateExtensions",
        "onCommand:teamEssentials.applyDefaultSettings",
        "onDebug:teamEssentials.debugStart",
        "onCommand:teamEssentials.debugStop",
        "workspaceContains:.vscode/team.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "jsonValidation": [
            {
                "fileMatch": "team.json",
                "url": "./team.json"
            }
        ],
        "commands": [
            {
                "command": "teamEssentials.changeWindowsShell",
                "title": "Team Essentials: Change Windows Shell"
            },
            {
                "command": "teamEssentials.filterExplorer",
                "title": "Team Essentials: Filter Explorer"
            },
            {
                "command": "teamEssentials.updateExtensions",
                "title": "Team Essentials: Update Team Extensions"
            },
            {
                "command": "teamEssentials.applyDefaultSettings",
                "title": "Team Essentials: Apply Default Settings"
            }
        ],
        "keybindings": [
            {
                "command": "teamEssentials.debugStop",
                "key": "shift+F5",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.39",
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "comment-json": "^1.1.3",
        "run-in-terminal": "0.0.3",
        "lodash": "^4.17.2"
    }
}