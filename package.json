{
  "name": "team-essentials",
  "displayName": "Team Essentials",
  "description": "Provides the essentials tools for any team.",
  "version": "0.2.4",
  "author": "SteveHartzog",
  "publisher": "SteveHartzog",
  "license": "MITNFA",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveHartzog/team-essentials"
  },
  "engines": {
    "vscode": "^1.7.1"
  },
  "categories": [
    "Linters",
    "Debuggers",
    "Other",
    "Keymaps"
  ],
  "activationEvents": [
    "onCommand:teamEssentials.changeWindowsShell",
    "onCommand:teamEssentials.filterExplorer",
    "onCommand:teamEssentials.updateExtensions",
    "onDebug:teamEssentials.debugStart",
    "onCommand:teamEssentials.debugStop",
    "workspaceContains:.vscode/team.json"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "team.json",
        "url": "./team.json"
      }
    ],
    "commands": [
      {
        "command": "teamEssentials.changeWindowsShell",
        "title": "Team Essentials: Change Windows Shell"
      },
      {
        "command": "teamEssentials.filterExplorer",
        "title": "Team Essentials: Filter Explorer"
      },
      {
        "command": "teamEssentials.updateExtensions",
        "title": "Team Essentials: Update Team Extensions"
      }
    ],
    "keybindings": [
      {
        "command": "teamEssentials.debugStop",
        "key": "shift+F5",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/lodash": "^4.14.39",
    "comment-json": "^1.1.3",
    "lodash": "^4.17.2",
    "run-in-terminal": "0.0.3"
  }
}